# Update dotfiles
function upd() { 
  CURRENT_DIR=$PWD
  cd $HOME/.dotfiles
  git pull
  git submodule init
  git submodule update

  if test -x "`which rake`"; then
    rake install
  else
    ./install
  fi
  git submodule foreach git checkout master
  git submodule foreach git pull origin master
  cd $CURRENT_DIR
}
